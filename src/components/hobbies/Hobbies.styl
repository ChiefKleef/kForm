.hobbies {
  position: relative;
  width: 100%;
  height: 100%;
  .hobbies-container {
    max-height: 132px;
    display: flex;
    flex-wrap: wrap;
    flex-direction: row
    justify-content: flex-start;
    align-items: center;
    overflow-y: auto;
  }
  .hobbies-container-active {
    margin-bottom: 5px;
  }
  .hobbies-container-inactive {
    margin-bottom: 0px;
  }
}