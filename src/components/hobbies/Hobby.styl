.hobby {
  font-family: 'Raleway', sans-serif;
  font-weight: 400;
  font-size: 13pt;
  height: 22px;
  width: max-content;
  padding-left: 10px;
  white-space: nowrap;
  .fa {
    color: rgb(150,150,150);
    margin-right: 5px;
    cursor: pointer;
    &:hover{
      color: white;
    }
    &:active{
      color: rgb(210,210,210);
    }
  }
}